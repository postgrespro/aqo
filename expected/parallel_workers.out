-- More complex query just to provoke errors
SELECT count(*) FROM
  (SELECT id FROM t WHERE id % 100 = 0 GROUP BY (id)) AS q1,
  (SELECT max(id) AS id, payload FROM t
    WHERE id % 101 = 0 GROUP BY (payload)) AS q2
WHERE q1.id = q2.id; -- Learning stage
